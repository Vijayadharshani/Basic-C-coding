#include<stdio.h>
#include<limits.h>
int main()
{
printf("Minimum value of unsigned integer is 0\n");
printf("Maximum value of unsigned integer is %u\n",UINT_MAX);
printf("Minimum value of unsigned character is 0\n");
printf("Maximum value of unsigned character is %d\n",UCHAR_MAX);
printf("Minimum value of unsigned long integer is 0\n");
printf("Maximum value of unsigned long integer is %lu\n",ULONG_MAX);
printf("Minimum value of unsigned long long integer is 0\n");
printf("Maximum value of unsigned long long integer is %llu\n",ULLONG_MAX);
printf("Minimum value of signed long long integer is %lld\n",LLONG_MIN);
printf("Maximum value of signed long long integer is %lld\n",LLONG_MAX);
printf("Minimum value of signed character is %d\n",CHAR_MIN);
printf("Maximum value of signed character is %d\n",CHAR_MAX);
printf("Minimum value of signed integer is %lld\n",LONG_MIN);
printf("Maximum value of signed  integer is %lld\n",LONG_MAX);
printf("Minimum value of integer is %d\n",INT_MIN);
printf("Maximum value of integer is %d\n",INT_MAX);
return 0;
}




